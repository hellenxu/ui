        <activity android:name=".viewpager2.ViewPager2TabLayoutActivity"/>

        <activity android:name=".dropdown.MainActivity"/>

        <activity android:name=".tabLayout.CoordinatorLayoutTabLayoutVP2" />

        <activity android:name=".tabLayout.EmptyActivity" />

        <activity android:name=".viewpager2.nestedscrolling.HomeNestedScrollingActivity" />

        <activity android:name=".drawing.SampleActivity" />

        <activity android:name=".softkeyboard.MainActivity" android:windowSoftInputMode="adjustResize" >
        <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".map.MainActivity" />

        <activity android:name=".tabLayout.MainActivity" />

        <activity android:name=".sequencelayout.SampleActivity" />

        <activity android:name=".constraintlayout.ConstraintSample" />

