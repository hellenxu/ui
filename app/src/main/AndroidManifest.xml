        <activity android:name="com.six.ui.sample.scrolling.ScrollingSample" />

        <activity android:name="com.six.ui.sample.recyclerview.MainActivity" />

        <activity android:name="com.six.ui.sample.ProgressWheelActivity" />

        <activity android:name="com.six.ui.sample.clock.ClockSample" />

        <activity android:name="com.six.ui.sample.colors.ColorMatrixSample" />

